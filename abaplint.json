{
    "global": {
      "files": "/src/**/*.*"
    },
    "dependencies": [
      {
        "url": "https://github.com/abaplint/deps",
        "folder": "/deps",
        "files": "/src/**/*.*"
      }
    ],
    "syntax": {
      "version": "v750",
      "errorNamespace": "^(Z|Y|LCL_|TY_|LIF_)"
    },
    "rules": {
      "exporting": true,
      "local_class_naming": {
        "exclude": [],
        "severity": "Error",
        "patternKind": "forbidden",
        "ignoreNames": [],
        "ignorePatterns": [],
        "local": "^LCL_.+$",
        "exception": "^LCX_.+$",
        "test": "^LTCL_.+$"
      },
      "method_parameter_names": {
        "exclude": [],
        "severity": "Error",
        "patternKind": "required",
        "ignoreNames": [],
        "ignorePatterns": [],
        "ignoreExceptions": true
      },
      "object_naming": {
        "exclude": [],
        "severity": "Error",
        "patternKind": "required",
        "ignoreNames": [],
        "ignorePatterns": [],
        "clas": "^Z",
        "intf": "^ZIF",
        "prog": "^Z",
        "fugr": "^Z",
        "tabl": "^Z",
        "ttyp": "^Z",
        "dtel": "^Z",
        "doma": "^Z",
        "msag": "^Z",
        "tran": "^Z",
        "enqu": "^EZ",
        "auth": "^Z",
        "pinf": "^Z",
        "idoc": "^Z",
        "xslt": "^Z",
        "ssfo": "^Z",
        "ssst": "^Z",
        "shlp": "^Z"
      },
      "unused_variables": true
    }
  }
